{
  "Id": "Policy8757577",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Stmt",
      "Action": [
        "s3:*"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:s3:::${s3_bucket_name}/*",
      "Principal": {
        "AWS": [
          "${account_id}"
        ]
      }
    }
  ]
}
